{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf .next node_modules pnpm-lock.yaml",
    "seed": "python -m api.seeds.seed",
    "api": "pip3 install -r requirements.txt && python -m api.index",
    "flask-dev": "cross-env FLASK_DEBUG=1 pip3 install -r requirements.txt && python -m flask --app api/index run -p 5328",
    "next-dev": "next dev",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "^15.3.1",
    "next": "^15.3.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "cross-env": "^7.0.3",
    "postcss": "8.4.31",
    "tailwindcss": "^4.1.10",
    "ts-silence-diagnostics": "^1.0.2"
  }
}
